= javascript_include_tag 'web/propositions'

.row
  .col-md-10
    = simple_form_for @proposition, url: { controller: 'web/admin/propositions', action: action }, html: { class: 'form-horizontal' } do |f|
      = f.input :text
      = f.input :begin_date
      = f.input :end_date
      = f.button :submit
