= javascript_include_tag 'web/cities'

.row
  .col-md-10
    = simple_form_for @city, url: { controller: 'web/admin/cities', action: action }, html: { class: 'form-horizontal' } do |f|
      = f.input :title, as: :string
      = f.button :submit
      = link_to t('helpers.back'), admin_cities_path, class: "btn"
