= javascript_include_tag 'web/blogs'

.row
  .col-md-10
    = simple_form_for @blog, url: { controller: 'web/admin/blogs', action: action }, html: { class: 'form-horizontal' } do |f|
      = f.input :title, as: :string
      = f.input :body
      = f.input :photo, as: :image_preview, input_html: { preview_version: :medium }
      = f.button :submit
