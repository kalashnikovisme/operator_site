    %meta{content: "", name: "description"}
    %meta{content: "", name: "author"}
    %title
      = t('company.name')
    / Bootstrap Core CSS
    = javascript_include_tag 'application'
    = stylesheet_link_tag :application
    = stylesheet_link_tag 'style'
    = stylesheet_link_tag 'bootstrap.min'
    = javascript_include_tag 'bootstrap.min'
    / Retina.js - Load first for faster HQ mobile images.
    = javascript_include_tag 'js/plugins/retina/retina.min.js'
    / Font Awesome
    = stylesheet_link_tag 'font-awesome/css/font-awesome.min.css', rel: 'stylesheet', type: 'text/css'
    / Default Fonts
    = stylesheet_link_tag "http://fonts.googleapis.com/css?family=Roboto:400,100,100italic,300,300italic,400italic,500,500italic,700,700italic,900,900italic", rel: "stylesheet", type: "text/css"
