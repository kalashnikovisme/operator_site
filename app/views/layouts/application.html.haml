    / Modern Style Fonts (Include these is you are using body.modern!)
    = stylesheet_link_tag "http://fonts.googleapis.com/css?family=Montserrat:400,700", rel: "stylesheet", type: "text/css"
    = stylesheet_link_tag "http://fonts.googleapis.com/css?family=Cardo:400,400italic,700", rel: "stylesheet", type: "text/css"
    / Vintage Style Fonts (Include these if you are using body.vintage!)
    = stylesheet_link_tag "http://fonts.googleapis.com/css?family=Sanchez:400italic,400", rel: "stylesheet", type: "text/css"
    = stylesheet_link_tag "http://fonts.googleapis.com/css?family=Cardo:400,400italic,700", rel: "stylesheet", type: "text/css"
    / Plugin CSS
    = stylesheet_link_tag "css/plugins/owl-carousel/owl.carousel.css", rel: "stylesheet", type: "text/css"
    = stylesheet_link_tag "css/plugins/owl-carousel/owl.theme.css", rel: "stylesheet", type: "text/css"
    = stylesheet_link_tag "css/plugins/owl-carousel/owl.transitions.css", rel: "stylesheet", type: "text/css"
    = stylesheet_link_tag "css/plugins/magnific-popup.css", rel: "stylesheet", type: "text/css"
    = stylesheet_link_tag "css/plugins/jquery.fs.wallpaper.css", rel: "stylesheet", type: "text/css"
    = stylesheet_link_tag "css/plugins/animate.css", rel: "stylesheet", type: "text/css"
